<mvc:View
        controllerName="testsapui5.controller.App"
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc">
    <Page title="{i18n>homePageTitle}">
        <headerContent>
            <Button
                    icon="sap-icon://hello-world"
                    press="onOpenDialog"/>
        </headerContent>
        <content>
            <mvc:XMLView viewName="testsapui5.view.HelloPanel"/>
            <mvc:XMLView viewName="testsapui5.view.InvoiceList"/>
        </content>
    </Page>
</mvc:View>